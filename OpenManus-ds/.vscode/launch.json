    "version": "0.2.0",
    "configurations": [
        {
            "type": "python",
            "name": "Python: Debug with Args",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "-batch", "64",
                "-lamb", "0.5"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1"
            }

        }
    ]
}