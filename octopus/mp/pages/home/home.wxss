.map {
  height: 100vh;
  width: 100vw;
}

.dialog-container {
  background-color: #000000;
  position: absolute;
  height: 0vh; /* 80vh */
  width: 100vw;
  top: 1000rpx;    /* 20vh */
}

.dialog-main {
  background-color: #808080;
  width: 100vw;
  height: 70vh;
  top: 800rpx;
  box-sizing: border-box;
  padding-bottom: 98rpx;
  z-index: 10;
}

.dialog-main .box-child {
  height: 100vh;   /* height: 20vh; */
  background-color: #ffffff;
  border-radius: 20rpx 20rpx 0 0;
  box-shadow: 0 -4rpx 16rpx 0 rgba(0,0,0,.08);
}

.dialog-main .box-child .show-all-btn {
  height: 32rpx;
  padding-top: 16rpx;
}

.dialog-main .box-child .show-all-btn .btn {
  background: rgba(0,0,0,.15);
  border-radius: 4rpx;
  height: 8rpx;
  margin: 0 auto;
  width: 64rpx;
}

.dialog-main .box-child .head {
  justify-content: space-between;
  padding: 23rpx 34rpx;
}

.dialog-main .box-child .head .city {
  align-items: center;
  display: inline-flex;
}

.dialog-main .box-child .head .arrow-down-icon {
  height: 40rpx;
  width: 40rpx;
}

.dialog-main .box-child .head .power-station-search {
  flex: 1;
  display: inline-flex;
  margin: -14rpx 8rpx -14rpx -20rpx;
}

.dialog-main .box-child .head .power-station-search .search-input-container {
  background: #fff;
  box-sizing: border-box;
  color: #242424;
  height: 60rpx;
  margin: 14rpx 32rpx;
  position: relative;
}

.dialog-main .box-child .head .power-station-search .search-input-container .search-input {
  background: #f8f8f8;
  border-radius: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 72rpx;
}

.dialog-main .box-child .activity {
  justify-content: space-between;
  padding: 23rpx 34rpx;
}

